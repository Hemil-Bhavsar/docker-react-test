{
    "AWSEBDockerrunVersion": "2",
    "containerDefinitions": [
        {
            "name": "docker-react-test",
            "image": "your-docker-image-name:latest",
            "essential": true,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/aws/elasticbeanstalk/your-environment-name",
                    "awslogs-region": "your-region",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}
